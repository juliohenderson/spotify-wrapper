{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "A Wrapper with unit tests to work of the Spotify Web Api",
  "main": "src/index.js",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src/**/*js ./examples/**/*js ./tests/**/*js",
    "prepush": "yarn lint && yarn test:coverage",
    "clear": "rimraf lib",
    "build": "yarn clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "yarn build -- --watch",
    "build:umd": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js",
    "build:umd:min": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.min.js -p",
    "build:all": "yarn build && yarn build:umd && yarn build:umd:min",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc yarn test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6",
    "babel-loader": "7.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-mocha": "^6.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "^3.0.4",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliohenderson/spotify-wrapper.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library",
    "spotify",
    "tests",
    "unit"
  ],
  "author": "JÃºlio Henderson <julio.henderson98@gmail.com> (https://linkedin.com/in/juliohenderson/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juliohenderson/spotify-wrapper/issues"
  },
  "homepage": "https://github.com/juliohenderson/spotify-wrapper#readme",
  "dependencies": {
    "node-fetch": "2.6.0"
  }
}
